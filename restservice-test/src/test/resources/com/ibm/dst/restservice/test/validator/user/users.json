{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "users",
    "description": "users",
    "type": "object",
    "properties":{
    	"users":{
    		"type":"array",
    		"items":{
    			"type":"object",
    			"properties":{
    				"id":{
    					"type":"integer"
    				},
    				"name":{
    					"type":"string"
    				},
    				"email":{
    					"type":"string"
    				}
    			},
    			"required":["id","name","email"]
    		}
    	}
    },
    "required":["users"]
}